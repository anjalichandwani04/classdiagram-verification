@startuml
class Customer {
    - name: String
    - address: String
    - customerID: String
    + getName(): String
    + getAddress(): String
    + updateAddress(newAddress: String): void
    + getCustomerID(): String
}

class Account {
    - balance: Double
    - accountNumber: String
    - accountType: String
    + getBalance(): Double
    + deposit(amount: Double): void
    + withdraw(amount: Double): void
    + getAccountNumber(): String
    + getAccountType(): String
}

class SavingsAccount {
    - interestRate: Double
    + calculateInterest(): Double
    + applyInterest(): void
}

class InvestmentAccount {
    - stocks: List<Stock>
    + buyStock(stock: Stock, quantity: int): void
    + sellStock(stock: Stock, quantity: int): void
    + getStocks(): List<Stock>
}

class Stock {
    - ticker: String
    - quantity: int
    - price: Double
    + getTicker(): String
    + getQuantity(): int
    + getPrice(): Double
    + updatePrice(newPrice: Double): void
}

class Commission {
    - rate: Double
    + calculateCommission(amount: Double): Double
    + getRate(): Double
}

Customer "1" -- "0..*" Account : Association
Account <|-- SavingsAccount : Inheritance
Account <|-- InvestmentAccount : Inheritance
InvestmentAccount "0..*" -- "0..*" Stock : Aggregation
Commission "0..*" -- "0..*" InvestmentAccount : Association
Stock "0..*" -- "0..*" Commission : Association
@enduml