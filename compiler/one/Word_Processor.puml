@startuml
class Document {
    +documentId
    +title
    +creationDate
    +lastModifiedDate
    +open()
    +save()
    +print()
}

class Page {
    +pageNumber
    +header
    +footer
    +addHeader(headerText)
    +addFooter(footerText)
}

class Sentence {
    +sentenceId
    +text
    +addWord(word)
    +addPunctuation(punctuation)
}

class Word {
    +wordId
    +text
    +addCharacter(character)
}

class Punctuation {
    +punctuationId
    +symbol
}

class Picture {
    +pictureId
    +imagePath
    +insert()
    +resize()
}

class Table {
    +tableId
    +numberOfRows
    +numberOfColumns
    +addRow()
    +addColumn()
}

class TableCell {
    +cellId
    +content
    +addText(text)
    +addPicture(picture)
}

Document "1" -- "1..*" Page : contains >
Page "1" -- "1..*" Sentence : contains >
Page "1" -- "1..*" Table : contains >
Page "1" -- "1..*" Picture : contains >
Sentence "1" -- "1..*" Word : contains >
Sentence "1" -- "1..*" Punctuation : contains >
Table "1" -- "1..*" TableCell : contains >
TableCell "1" -- "1" Table : belongs to >
Picture "1" -- "1" Page : belongs to >
Word "1" -- "1" Sentence : belongs to >
Punctuation "1" -- "1" Sentence : belongs to >
@enduml
